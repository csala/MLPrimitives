{
    "name": "mlprimitives.custom.timeseries_preprocessing.simple_rolling_windows",
    "contributors": [
        "Carles Sala <csala@csail.mit.edu>",
        "Alexander Geiger <geigera@mit.edu>"
    ],
    "description": "Create rolling window sequences out of timeseries data.",
    "classifiers": {
        "type": "preprocessor",
        "subtype": "feature_extractor"
    },
    "modalities": [
        "timeseries"
    ],
    "primitive": "mlprimitives.custom.timeseries_preprocessing.simple_rolling_windows",
    "produce": {
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "y",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "window_size": {
                "type": "int",
                "default": 50
            },
            "target_size": {
                "type": "int",
                "default": 1
            }
        }
    }
}
